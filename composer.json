{
    "name": "viloveul/cms",
    "description": "Viloveul CMS.",
    "type": "project",
    "license": "MIT",
    "authors": [{
        "name": "Fajrul Akbar Zuhdi",
        "email": "fajrulaz@gmail.com"
    }],
    "keywords": ["project", "api", "api", "zafex", "viloveul", "fajrulaz"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "viloveul/auth": "1.0.*",
        "viloveul/cache": "1.0.*",
        "viloveul/config": "1.0.*",
        "viloveul/console": "1.0.*",
        "viloveul/container": "1.0.*",
        "viloveul/event": "1.0.*",
        "viloveul/http": "1.0.*",
        "viloveul/kernel": "1.0.*",
        "viloveul/media": "1.0.*",
        "viloveul/middleware": "1.0.*",
        "viloveul/pagination": "1.0.*",
        "viloveul/router": "1.0.*",
        "viloveul/support": "1.0.*",
        "viloveul/validation": "1.0.*",
        "vlucas/phpdotenv": "2.5.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require-dev": {
        "codeception/codeception": "2.5.*"
    },
    "scripts": {
        "clearall": [
            "rm -f config/private.pem config/public.pem"
        ],
        "bootstrap": [
            "mkdir -p public/uploads",
            "touch .env"
        ],
        "genrsakey": [
            "openssl genrsa -out config/private.pem -aes256 4096",
            "openssl rsa -pubout -in config/private.pem -out config/public.pem"
        ],
        "post-create-project-cmd": [
            "@bootstrap",
            "@genrsakey"
        ]
    }
}
